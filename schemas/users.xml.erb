<% require 'openssl' %>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users SYSTEM "users.dtd" >
<users>
  <% 1.upto 100 do |i| %>
    <user id="U<%= i %>">
      <login>usr<%= i %></login>
      <password><%= OpenSSL::Digest::SHA512.hexdigest('pw' + i.to_s) %></password>
      <firstname>firstname<%= i %></firstname>
      <surname>surname<%= i %></surname>
      <mail>mail<%= i %>@kikoo.com</mail>
      <phone>phone<%= i %></phone>
      <avatar>avatarpath</avatar>
    </user>

<% end %>
</users>
